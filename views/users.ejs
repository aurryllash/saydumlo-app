<%- include('./partials/header')%>
<section class="container">
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Role</th>
          </tr>
        </thead>
        <tbody>
            <% users.forEach((user, index) => { %>    
          <tr>
            <th scope="row">1</th>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.role %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
</section>


</body>
</html>